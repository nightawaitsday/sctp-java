package sg.edu.ntu.fnbapi.service;

// import sg.edu.ntu.fnbapi.entity.Consumer; 
import sg.edu.ntu.fnbapi.entity.Restaurant; 

public class FavouriteDetails {

    private Long consumerId;
    private Restaurant restaurant;

    public FavouriteDetails(Long consumerId, Restaurant restaurant) {
        this.consumerId = consumerId;
        this.restaurant = restaurant;
    }

    public Long getConsumerId() {
        return consumerId;
    }

    public void setConsumerId(Long consumerId) {
        this.consumerId = consumerId;
    }

    public Restaurant getRestaurant() {
        return restaurant;
    }

    public void setRestaurant(Restaurant restaurant) {
        this.restaurant = restaurant;
    }
}
